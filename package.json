{
  "name": "quark-ui-rate",
  "version": "0.5.4",
  "keywords": [
    "frontend",
    "quark",
    "web components",
    "rate"
  ],
  "scripts": {
    "dev": "pnpm run start",
    "start": "vite",
    "build": "rimraf lib && vite build --config vite.config.build.ts",
    "prepublishOnly": "pnpm run build"
  },
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/types/src/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "lodash-es": "^4.17.21",
    "quarkc": "^1.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.5",
    "autoprefixer": "^10.4.14",
    "less": "^4.1.3",
    "postcss": "^8.4.24",
    "rimraf": "^5.0.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.0"
  },
  "engines": {
    "node": ">=14.18.0"
  }
}
